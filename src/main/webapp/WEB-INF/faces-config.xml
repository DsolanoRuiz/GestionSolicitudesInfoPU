<?xml version='1.0' encoding='UTF-8'?>

<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/alta_interesado.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>altaInteres</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/lista_interesados.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>listaInteres</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/solicitud_info.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>SolicInfo</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/lista_solicitudes.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>listaSolic</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
